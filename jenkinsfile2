Pipeline {
  agent any 
  parameters {
    properties(
      [parameters(
        [string(defaultValue: '', 
          description: '', 
          name: 'Access_Key', 
          trim: false), 
        string(defaultValue: '', 
        description: '', 
        name: 'Secrate_key', 
        trim: false)])
      ])
  }
  Environment {
    
  }
    stages {
      stage ('build') {
        steps {
          sh 'mvn clean install package'
          archiveArtifacts artifacts: '/target/*.war'
        }
      }
      stage ('AWS Access') {
        steps{
          sh ' AWS configure ${Access_Key} ${Secrate_key} '
        }
      }
    }
}
